{
  "name": "tessla",
  "main": "./lib/tessla",
  "version": "1.0.0",
  "description": "Package providing IDE functions for C code and the TeSSLa temporal logic",
  "keywords": [
    "tessla",
    "tessla-ide",
    "ide"
  ],
  "activationCommands": {
    "atom-workspace": "tessla:toggle"
  },
  "repository": "https://github.com/dmlux/TeSSLa",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.3.1",
    "jquery": "^3.1.1",
    "scan-folder": "^1.0.0"
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "package-deps": [
    "tool-bar",
    "linter-gcc"
  ]
}
